{
  "$schema": "http://json.schemastore.org/template",
  "author": "NETWORG",
  "classifications": [ "TALXIS", "Dataverse", "Script Library" ],
  "identity": "TALXIS.DevKit.Templates.Dataverse.ScriptLibrary",
  "name": "TALXIS Script Library",
  "shortName": "pp-script-library",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "examplesourcename",
  "preferNameDirectory": true,
  "symbols": {
    "PublisherPrefix": {
      "type": "parameter",
      "datatype": "string",
      "replaces": "examplepublisherprefix",
      "FileRename": "examplepublisherprefix",
      "defaultValue": "talxis",
      "description": "Publisher prefix for your solution"
    },
    "ModuleName": {
      "type": "parameter",
      "datatype": "string",
      "replaces": "ExampleModuleName",
      "defaultValue": "MyModule",
      "description": "Name of your script module"
    },
    "EntityName": {
      "type": "parameter",
      "datatype": "string",
      "replaces": "exampleentityname",
      "FileRename": "exampleentityname",
      "defaultValue": "account",
      "description": "Logical name of entity"
    }
  },
  "sources": [
    {
      "modifiers": [
        {
          "exclude": [
            ".vs/**/*",
            ".vscode/**/*",
            "bin/**/*",
            "obj/**/*",
            "node_modules/**/*",
            "dist/**/*"
          ]
        }
      ]
    }
  ],
  "postActions": [
    
  ]
}
