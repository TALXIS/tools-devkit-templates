{
  "symbols": {
    "example-symbol": {
      "type": "parameter",
      "dataType": "string",
      "replaces": "ExampleValue",
      "defaultValue": "MyValue",
      "fileRename": {
        "from": "example",
        "to": "custom"
      }
    }
  },
  "stages": {
    "before": [
      {
        "description": "Выполняется до основной обработки шаблона",
        "actionId": "setup",
        "args": {
          "key": "value"
        }
      }
    ],
    "during": [
      {
        "description": "Выполняется во время основной обработки шаблона",
        "actionId": "process",
        "condition": "(symbol.example-symbol == 'MyValue')"
      }
    ],
    "after": [
      {
        "description": "Выполняется после основной обработки шаблона",
        "actionId": "cleanup",
        "manual": true
      }
    ]
  }
} 